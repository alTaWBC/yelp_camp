<% layout('layouts/boilerplate.ejs') %>
<div class="row">
    <div class="col-8 offset-2">
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading"><%= error.statusCode %> Error Status Code</h4>
            <p><%= error.message %></p>
            <hr />
            <p class="mb-0"><%= error.stack %> ></p>
        </div>
    </div>
</div>
